{
  "extends": ["tslint:recommended", "tslint-react", "tslint-react-hooks"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "no-shadowed-variable": false,
    "no-empty-interface": false,
    "no-empty": false,
    "no-console": false
  }
}
